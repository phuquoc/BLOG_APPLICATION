<!DOCTYPE html>
<html>
  <head>
    <title>Nhap</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="wrapper container-fluid">
      <div class="header row">
        <div class="col-md-12">
          <nav class="navbar navbar-light bg-faded">
            <a class="navbar-brand" href="#">Blog Application By Nho and Quoc</a>
            <ul class="nav navbar-nav">
              <li class="nav-item active">
                <a class="nav-link" href="#">Home <span class="sr-only">(current)</span></a>
              </li>
              <li class="nav-item">

                <%= link_to  "Hello", hello_path, class: 'nav-link' %>
              <!--  <a class="nav-link" href="#">Say Hello</a> -->
              </li>
              <li class="nav-item">
                <%= link_to "Aritcles", blogs_path, class: 'nav-link' %>
              <!--  <a class="nav-link" href="/blogs">Article</a> -->
              </li>

            </ul>
           </nav>
        </div>
      </div>
      <div class="content row">
        <div class="sidebar col-md-3">
          side bar
        </div>
        <div class="main-content col-md-9 ">
          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
